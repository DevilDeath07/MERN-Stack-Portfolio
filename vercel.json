{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "server/.env"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/index.js"
        }
    ]
}